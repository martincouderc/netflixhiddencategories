{
"name":"Netflix Hidden Categories",
"description":"Display all hidden categories in the Netflix menu",
"version":"1",
"manifest_version":2,
"content_scripts": [
    {
      "matches": ["http://www.netflix.com/*","https://www.netflix.com/*"],
      "css": ["nhc-style.css"],
      "js": ["jquery-2.2.0.min.js", "nhc-script.js"]
    }
  ]
}